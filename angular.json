{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",


  "projects": {
    "ngx-generic-tools": {
      "projectType": "library",
      "root": "projects/ngx-generic-tools",
      "sourceRoot": "projects/ngx-generic-tools/src",
      "prefix": "gt",
      "architect": {
        "build": {

          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-generic-tools/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-generic-tools/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-generic-tools/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },


        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ngx-generic-tools/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },


    "sample-project": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/sample-project",
      "sourceRoot": "projects/sample-project/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/sample-project",
            "index": "projects/sample-project/src/index.html",
            "main": "projects/sample-project/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/sample-project/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/sample-project/src/favicon.ico",
              "projects/sample-project/src/assets"
            ],
            "styles": [
              "projects/sample-project/src/styles.scss",
              "@angular/material/prebuilt-themes/indigo-pink.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "sample-project:build:production"
            },
            "development": {
              "browserTarget": "sample-project:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sample-project:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/sample-project/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/sample-project/src/favicon.ico",
              "projects/sample-project/src/assets"
            ],
            "styles": [
              "projects/sample-project/src/styles.scss",
              "@angular/material/prebuilt-themes/indigo-pink.css"
            ],
            "scripts": []
          }
        }
      }
    },

  
    "testing-project": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/testing-project",
      "sourceRoot": "projects/testing-project/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/testing-project",
            "index": "projects/testing-project/src/index.html",
            "main": "projects/testing-project/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/testing-project/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/testing-project/src/favicon.ico",
              "projects/testing-project/src/assets"
            ],
            "styles": [
              "projects/testing-project/src/styles.scss",
              "@angular/material/prebuilt-themes/indigo-pink.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "testing-project:build:production"
            },
            "development": {
              "browserTarget": "testing-project:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "testing-project:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/testing-project/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/testing-project/src/favicon.ico",
              "projects/testing-project/src/assets"
            ],
            "styles": [
              "projects/testing-project/src/styles.scss",
              "@angular/material/prebuilt-themes/indigo-pink.css"
            ],
            "scripts": []
          }
        }
      }
    }
  }

}
